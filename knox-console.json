    "homepage": "https://github.com/KNOXDEV",
    "description": "KNOX's prefered custom Powershell style and prompt",
    "license": "Unlicense",
    "version": "1.0",
    "url": [
    	"https://gist.githubusercontent.com/KNOXDEV/70a23720e936290cbad52e11fb020285/raw/c2fb14512fe80a195f8cb480718565a059605079/console-mustang.json",
    	"https://gist.githubusercontent.com/KNOXDEV/84da0469eea9c9abaa2d60301fde2383/raw/f42a7688866eacb08d9d5833516280356b9fb8b0/console-prompt.json"
    ],
    "hash": [
    	"507A6D5865EC2B9FE995A859D504BCB61C4546C130029C05CDB67F38D77193D9",
    	"75889A4FBB4D3B9B0CD316E41B1D1FC8103D3EF2A4D01229C2A986A17F817DCE"
    ],
    "depends": ["pshazz", "concfg"],
    "installer": {
        "script": [
        	"concfg tokencolor enable",
        	"concfg import \"$dir\\console-mustang.json\" -n",
        	"cp \"$dir\\console-prompt.json\" \"$($env:SCOOP, \"$env:USERPROFILE\\scoop\" | Select-Object -first 1)\\apps\\pshazz\\current\\themes\"",
        	"pshazz use console-prompt"
        ]
    },
    "uninstaller": {
        "script": [
        	"concfg tokencolor disable",
        	"concfg import powershell-defaults -n",
        	"pshazz use microsoft"
        ]
    }
}